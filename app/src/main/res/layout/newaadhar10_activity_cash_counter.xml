                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:gravity="center"
                            android:text="2000"
                            android:textColor="@color/black"
                            android:textSize="25dp" />

                        <ImageView
